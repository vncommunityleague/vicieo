{
  "name": "@vncommunityleague/vietnam-community-league",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check-format": "prettier --ignore-path=../../.prettierignore --check .",
    "check-types": "tsc --pretty --noEmit",
    "check-all": "pnpm lint && pnpm check-format && pnpm check-types && pnpm build",
    "lint": "prettier --ignore-path=../../.prettierignore --plugin-search-dir . --check . && eslint .",
    "format": "prettier --ignore-path=../../.prettierignore --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "test": "pnpm run test:integration && pnpm run test:unit"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/svelte-fontawesome": "^0.2.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/parser": "^5.45.0",
    "@vncommunityleague/eslint-config": "workspace:^",
    "@vncommunityleague/tsconfig": "workspace:^",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte": "^2.30.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  }
}
